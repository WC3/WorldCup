<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.edu.shu.worldcup.domain">
	<class name="Team" table="team_table">
		<id name="id" column="team_id" type="integer">
			<generator class="identity" />
		</id>
		<property name="name" column="team_name" type="string"
			not-null="true" unique="true" />
		<property name="group" column="team_group" type="string"
			not-null="true" />
		<property name="event" column="team_event" type="integer"
			not-null="true" />
		<property name="goals" column="team_goals" type="integer"
			not-null="true" />
		<property name="conceded" column="team_conceded" type="integer"
			not-null="true" />
		<property name="assists" column="team_assists" type="integer"
			not-null="true" />
		<property name="shots" column="team_shots" type="integer"
			not-null="true" />
		<property name="shooting" column="team_shooting" type="integer"
			not-null="true" />
		<property name="fighting" column="team_fighting" type="integer"
			not-null="true" />
		<property name="steals" column="team_steals" type="integer"
			not-null="true" />
		<property name="intercept" column="team_intercept" type="integer"
			not-null="true" />
		<property name="foul" column="team_foul" type="integer"
			not-null="true" />
		<property name="closure" column="team_closure" type="integer"
			not-null="true" />
		<property name="offside" column="team_offside" type="integer"
			not-null="true" />
		<property name="passing" column="team_passing" type="integer"
			not-null="true" />
		<property name="possession" column="team_possession" type="string"
			not-null="true" />
		<property name="yellow" column="team_yellow" type="integer"
			not-null="true" />
		<property name="red" column="team_red" type="integer"
			not-null="true" />
		<property name="corner" column="team_corner" type="integer"
			not-null="true" />
	</class>
</hibernate-mapping>