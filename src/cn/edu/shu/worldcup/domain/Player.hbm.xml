<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.edu.shu.worldcup.domain">
	<class name="Player" table="player_table">
		<id name="id" column="player_id" type="integer">
			<generator class="identity" />
		</id>
		<property name="name" column="player_name" type="string"
			not-null="true" unique="true" />
		<property name="team" column="player_team" type="string"
			not-null="true" />
		<property name="appearance" column="player_appearance" type="integer"
			not-null="true" />
		<property name="time" column="player_time" type="integer"
			not-null="true" />
		<property name="goals" column="player_goals" type="integer"
			not-null="true" />
		<property name="penalties" column="player_penalties" type="integer"
			not-null="true" />
		<property name="assists" column="player_assists" type="integer"
			not-null="true" />
		<property name="shots" column="player_shots" type="integer"
			not-null="true" />
		<property name="shooting" column="player_shooting" type="integer"
			not-null="true" />
		<property name="passing" column="player_passing" type="integer"
			not-null="true" />
		<property name="biography" column="player_biography" type="integer"
			not-null="true" />
		<property name="corner" column="player_corner" type="integer"
			not-null="true" />
		<property name="offside" column="player_offside" type="integer"
			not-null="true" />
		<property name="ball" column="player_ball" type="integer"
			not-null="true" />
		<property name="foul" column="player_foul" type="integer"
			not-null="true" />
		<property name="closure" column="player_closure" type="integer"
			not-null="true" />
		<property name="yellow" column="player_yellow" type="integer"
			not-null="true" />
		<property name="red" column="player_red" type="integer"
			not-null="true" />
	</class>
</hibernate-mapping>